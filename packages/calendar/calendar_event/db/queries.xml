<queries id="calendar_event">

	<query id="deleteCalendarEvent">
		<!-- Deletes calendar even with the specified assetid -->
        <primary>
            <delete>
                <where>
                    <equal table="sq_cal_date_val" column="assetid">:assetid</equal>
                </where>
            </delete>
        </primary>
	</query>

</queries>
