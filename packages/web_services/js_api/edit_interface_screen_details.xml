		<field attribute="trash_asset">
			<boolean true_text="yes" false_text="no" />
		</field>
	</section>	
    <section name="Metadata Functions" write_access="write:attributes">
		<field attribute="get_metadata">
			<boolean true_text="yes" false_text="no" />
		</field>
		<field attribute="set_metadata">
			<boolean true_text="yes" false_text="no" />
		</field>
	</section>	
