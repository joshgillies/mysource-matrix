<contents>
	<section name="Send to a Friend">
		<field attribute="staf_from_address">
			<display_name>From Email Address</display_name>
			<note>Address that the Message is from.</note>
			<text width="20" height="1" />
		</field>
		<field attribute="staf_subject">
			<display_name>Subject</display_name>
			<note>Subject of the email.</note>
			<text width="20" height="1" />
		</field>
		<field attribute="staf_message">
			<display_name>Message</display_name>
			<note>The Body of the email.</note>
			<text width="40" height="10" />
		</field>
	</section>

</contents>