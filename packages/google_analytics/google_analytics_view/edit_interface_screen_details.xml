<contents>
	<section name="Details" write_access="write:attributes" limbo_access="1">
		<field attribute="name">
			<display_name>View Name</display_name>
			<note />
			<text width="35" height="1" />
		</field>
		<field attribute="short_name">
			<display_name>View Short Name</display_name>
			<note />
			<text width="25" height="1" />
		</field>
	</section>
    <section name="Connector and View" write_access="write:menu">
        <field>
            <display_name>Select Connector</display_name>
            <note></note>
            <function_call>
                <paint_function   name="paintSelectConnector" />
                <process_function name="processSelectConnector" />
            </function_call>
        </field>
        <field>
            <display_name>Select View</display_name>
            <note></note>
            <function_call>
                <paint_function   name="paintSelectView" />
                <process_function name="processSelectView" />
            </function_call>
        </field>
    </section>
</contents>
