<contents>
	<section name="Details" write_access="write:attributes" limbo_access="1">
        <field attribute="name">
            <display_name>Name</display_name>
            <note />
            <text width="100" height="1" />
        </field>
		<field>
			<function_call>
				<paint_function		name="paintTestConnection" />
				<process_function	name="processTestConnection" />
			</function_call>
		</field>
		<field attribute="url">
			<display_name>URL</display_name>
			<note>The WSDL URL of the Sharepoint List Web Service. Example: http://webURL/_vti_bin/Lists.asmx?wsdl</note>
			<text width="100" height="1"/>
		</field>
		<field attribute="username">
			<display_name>Username</display_name>
			<note>The username used to connect to Sharepoint</note>
			<text width="100" height="1"/>
		</field>
		<field attribute="password">
			<display_name>Password</display_name>
			<note>The password used to connect to Sharepoint</note>
			<password width="100" height="1"/>
		</field>
		<field attribute="authentication_type">
			<display_name>Authentication Method</display_name>
			<selection/>
		</field>
	</section>
</contents>
