<contents>
	<section name="Import Directories" write_access="write:attributes">
		<field attribute="import_dir">
			<text width="30" height="1" />
		</field>
		<field attribute="max_file_size">
			<int width="30" height="1" />
		</field>
	</section>
	<section name="Import Configuration" write_access="write:attributes">
		<field>
			<function_call>
				<paint_function   name="paintHeadings"   />
				<process_function name="processHeadings" />
			</function_call>
		</field>
		<field attribute="create_css_file" write_access="write:attributes">
			<boolean true_text="yes" false_text="no" />
		</field>
	</section>
	<section name="Upload" write_access="write:attributes">
		<field>
			<function_call>
				<paint_function   name="paintHtmlUpload"   />
				<process_function name="processHtmlUpload" />
			</function_call>
		</field>
	</section>
</contents>
