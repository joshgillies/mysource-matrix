<contents>
	<section name="General Category Settings">
		<field write_access="write:attributes" attribute="no_category_text">
			<display_name>Not Selected Text</display_name>
			<note />
			<text width="20" />
		</field>
	</section>
	<section name="Select Existing Category" read_access="write:" write_access="write:attributes">
		<field>
			<display_name>Configure Search Category</display_name>
			<note />
			<function_call>
				<paint_function name="paintChooseCategory" />
				<process_function name="processChooseCategory" />
			</function_call>
		</field>
	</section>
	<section name="Search Category Management" write_access="write:attributes">
		<field>
			<display_name>Category Name</display_name>
			<note />
			<function_call>
				<paint_function name="paintCustomiseCategory" />
				<process_function name="processCustomiseCategory" />
			</function_call>
		</field>
	</section>
</contents>
