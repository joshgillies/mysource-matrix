<asset_info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../asset.xsd">
	<package>search</package>
	<type_code>search_page</type_code>
	<version>0.22</version>
	<name>Search Page</name>
	<description>Search Page asset; provides a front-end interface to the search manager so a user can search for assets within a MySource Matrix system.</description>
	<instantiable>yes</instantiable>
	<allowed_access>backend_user</allowed_access>
	<parent_type>listing_engine</parent_type>
	<requirements>
		<requirement type="asset">
			<code>listing_engine</code>
			<version>0.15</version>
		</requirement>
		<requirement type="asset">
			<code>search_manager</code>
			<version>0.5</version>
		</requirement>
	</requirements>
	<flash_menu_path>Search</flash_menu_path>
</asset_info>
