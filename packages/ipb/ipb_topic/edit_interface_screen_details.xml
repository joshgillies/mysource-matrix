<contents>
	<section name="Details" write_access="write:attributes" limbo_access="1">
		<field attribute="name">
			<text width="40" height="1" />
		</field>
		<field attribute="description">
			<text width="40" height="2" />
		</field>
		<field attribute="posts">
			<int width="5" />
		</field>
		<field>
			<function_call>
				<paint_function   name="paintStarterName" />
				<process_function name="processStarterName" />
			</function_call>
		</field>
		<field attribute="start_date">
			<text width="40" height="1" />
		</field>
		<field>
			<function_call>
				<paint_function   name="paintLastPosterName" />
				<process_function name="processLastPosterName" />
			</function_call>
		</field>
		<field attribute="last_post">
			<text width="40" height="1" />
		</field>
	</section>
</contents>
