<contents>
	<section name="Details" write_access="write:attributes" limbo_access="1">
		<field>
			<function_call>
				<paint_function   name="paintAuthor"   />
				<process_function name="processAuthor" />
			</function_call>
		</field>
		<field attribute="post_date">
			<text width="40" height="5" />
		</field>
		<field attribute="post_title">
			<text width="40" height="5" />
		</field>
		<field attribute="post_parent">
			<text width="40" height="5" />
		</field>
	</section>
	<section name="Content" write_access="write:attributes" limbo_access="1">
		<field>
			<function_call>
				<paint_function   name="paintPostContent"   />
				<process_function name="processPostContent" />
			</function_call>
		</field>
	</section>
</contents>
