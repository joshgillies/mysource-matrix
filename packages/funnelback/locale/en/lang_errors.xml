<errors>
	<!-- Funnelback Search Page -->
	<error code="FNB0001">Unable to contact the Funnelback server (%s)</error>
	<error code="FNB0002">Invalid response from the Funnelback server</error>
	<error code="FNB0003">No Funnelback server or collection is configured</error>
	<error code="FNB0004">Cannot find the %s bodycopy</error>
	<error code="FNB0005">Bodycopy (%s) already exists</error>
	<error code="FNB0006">Unable to delete Bodycopy (%s)</error>

	<!-- Funnelback Manager -->
	<error code="FNB0010">Unable to create the Funnelback Manager</error>
	<error code="FNB0011">The path provided does not look like a valid install of Funnelback</error>
	<error code="FNB0012">The Funnelback installation has incorrect permissions</error>
	<error code="FNB0013">Unable to create the collection path</error>
	<error code="FNB0014">Invalid Password: Incorrect user or password, correct for proper use</error>

	<!-- Scripts -->
	<error code="FNB0019">Lockfile found. Scripts cannot be run concurrently.</error>

	<!-- Funnelback HIPO Rebuild Cache -->
	<error code="FNB0020">Unable to load Funnelback Manager, Funnelback Manager is not installed</error>
	<error code="FNB0022">Unable to initialise Funnelback Rebuild Cache HIPO, invalid root asset ID specified (%s)</error>

	<!-- Funnelback HIPO Reindex -->
	<error code="FNB0030">Unable to reindex</error>
	<error code="FNB0031">Indexing failed</error>
	<error code="FNB0032">Unable to make index live</error>
</errors>
