<contents>
	<section name="Details" write_access="write:attributes" limbo_access="1">
		<field attribute="name">
			<text width="35" height="1" />
		</field>
	</section>
	<section name="Connection Setup" write_access="write:attributes">
		<field attribute="user">
			<text width="35" height="1" />
		</field>
		<field attribute="password">
			<password width="35"/>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintDSN"   />
				<process_function name="processDSN" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintDSNAlone"   />
				<process_function name="processDSNAlone" />
			</function_call>
		</field>
	</section>
	<section name="Use a Connector Asset" write_access="write:links">
		<field>
			<function_call>
				<paint_function   name="paintConnectorLink"   />
				<process_function name="processConnectorLink" />
			</function_call>
		</field>
	</section>
	<section name="Diagnostics">
		<field>
			<function_call>
				<paint_function   name="paintDebugInfo"   />
			</function_call>
		</field>
	</section>
</contents>
