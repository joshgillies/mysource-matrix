<contents>
	<section name="Default Formats" limbo_access="0">
		<field>
			<function_call>
				<paint_function   name="paintDefaultFormatLinks"   />
				<process_function name="processDefaultFormatLinks" />
			</function_call>
		</field>
	</section>
	<section name="Group Formats" limbo_access="0">
		<field>
			<function_call>
				<paint_function name="paintGroupFormatLinks" />
			</function_call>
		</field>
	</section>
	<section name="Customised Asset Type Formats" write_access="write:links" limbo_access="0">
		<field>
			<function_call>
				<paint_function   name="paintTypeFormats"   />
				<process_function name="processTypeFormats" />
			</function_call>
		</field>
	</section>
	<section name="Customised List Position Formats" write_access="write:links" limbo_access="0">
		<field>
			<function_call>
				<paint_function   name="paintPositionFormats"   />
				<process_function name="processPositionFormats" />
			</function_call>
		</field>
	</section>
</contents>
