	<section name="Unavailable Content">
		<field attribute="display_error">
			<display_name>Display Error Message</display_name>
			<note>Choose whether you want the error message displayed.</note>
		</field>
		<field attribute="error_message">
			<display_name>Error Message</display_name>
			<note>This message is displayed if an error occurs while capturing remote content.</note>
		</field>
