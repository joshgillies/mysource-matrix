<contents>
	<section name="General" write_access="write:attributes">
		<field attribute="print">
			<boolean true_text="yes" false_text="no" />
		</field>
		<field attribute="cache">
			<boolean true_text="yes" false_text="no" />
		</field>
		<field attribute="cache_globally">
			<boolean true_text="yes" false_text="no" />
		</field>
	</section>
	<section name="Condition Settings" write_access="write:attributes">
		<field>
			<function_call>
				<paint_function name="paintConditionType" />
				<process_function name="processConditionType" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function name="paintConditionDetails" />
				<process_function name="processConditionDetails" />
			</function_call>
		</field>
	</section>
</contents>