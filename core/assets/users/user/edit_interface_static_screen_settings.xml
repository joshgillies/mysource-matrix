<contents>
	<section name="Versioning">
		<field code="version_current_version">
			<display_name>Current Version</display_name>
			<note />
		</field>
		<field code="version_update_version">
			<display_name>Update Major Version ?</display_name>
			<note>Check the box to increment the major version number of this asset</note>
		</field>
	</section>
	<section name="Dates">
		<field code="date_created_date">
			<display_name>Created Date</display_name>
			<note>Specify the date and user for the created date</note>
		</field>
		<field code="date_published_date">
			<display_name>Published Date</display_name>
			<note>Specify the date and user for the published date</note>
		</field>
	</section>
	<section name="SSL">
		<field code="force_secure">
			<display_name>Force Secure</display_name>
			<note>Force that this asset can only be viewed through an Secure (SSL) connection</note>
		</field>
	</section>
	<section name="Designs">
		<field code="designs" />
	</section>
	<section name="Morphing">
		<field code="morph_new_type">
			<display_name>New Type</display_name>
			<note />
		</field>
	</section>
</contents>