<contents>
	<section name="Clear Cache" read_access="write:attributes" write_access="write:attributes">
		<field>
			<display_name>Choose Asset</display_name>
			<note />
			<function_call>
				<paint_function name="paintClearCache" />
				<process_function name="processClearCache" />
			</function_call>
		</field>
	</section>
	<section name="Clear System Wide Cache" read_access="write:attributes" write_access="write:attributes">
		<field>
			<function_call>
				<paint_function name="paintSystemWideClearCache" />
				<process_function name="processSystemWideClearCache" />
			</function_call>
		</field>
	</section>
</contents>
