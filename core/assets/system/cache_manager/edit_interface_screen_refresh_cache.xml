<contents>
	<section name="Refresh Cache" write_access="write:attributes">
		<field>
			<function_call>
				<paint_function name="paintAddJob" />
				<process_function name="processAddJob" />
			</function_call>
		</field>
		<field show_if="cronConfigured">
			<function_call>
				<paint_function name="paintCurrentJobs" />
				<process_function name="processCurrentJobs" />
			</function_call>
		</field>
	</section>
</contents>
