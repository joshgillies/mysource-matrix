	<section name="Select Asset Type">
		<field>
			<display_name>Configure Type Code</display_name>
			<note />
			<function_call>
				<paint_function name="paintChooseTypeCode" />
				<process_function name="processChooseTypeCode" />
			</function_call>
		</field>
