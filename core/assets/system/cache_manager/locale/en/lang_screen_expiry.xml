	<section name="Type Code Cache Management">
		<field>
			<display_name>Matrix Caching Status</display_name>
			<note />
			<function_call>
				<paint_function name="paintCustomiseTypeCodeCaching" />
				<process_function name="processCustomiseTypeCodeCaching" />
			</function_call>
		</field>
	</section>
</contents>
