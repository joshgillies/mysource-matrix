<contents>
	<section name="Running Status">
		<field attribute="running">
			<display_name>Running</display_name>
			<note/>
		</field>
		<field>
			<display_name>Last Run</display_name>
			<note/>
			<function_call>
				<paint_function name="paintLastRun" />
			</function_call>
		</field>
		<field attribute="run_check">
			<display_name>Run Check</display_name>
			<note>The number of consecutive times the cron's run has been cancelled due to another instance already running</note>
		</field>
	</section>
	<section name="Dead Lock Status" />
</contents>
