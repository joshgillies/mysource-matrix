<contents>
	<section name="CSS Settings" write_access="write:attributes">
		<field attribute="edit_css">
			<boolean true_text="yes" false_text="no" />
		</field>
	</section>
	<section name="Edit Layouts">
		<field read_access="write:" write_access="write:">
			<function_call>
				<paint_function		name="paintEditLayouts" />
				<process_function	name="processEditLayouts" />
			</function_call>
		</field>
	</section>
	<section name="Add A Layout">
		<field read_access="write:" write_access="write:">
			<function_call>
				<paint_function		name="paintAddLayout" />
				<process_function	name="processAddLayout" />
			</function_call>
		</field>
	</section>
</contents>