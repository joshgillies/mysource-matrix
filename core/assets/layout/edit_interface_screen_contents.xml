<contents>
	<section name="Simple Edit Keywords">
		<field keyword="limbo_keywords">
			<function_call>
				<paint_function   name="paintLimboKeywords"   />
				<process_function name="processLimboKeywords" />
			</function_call>
		</field>		
	</section>
	<section name="Contents" write_access="write:content" limbo_access="1">
		<field keyword="contents">
			<function_call>
				<paint_function   name="paintLayout"   />
				<process_function name="processLayout" />
			</function_call>
		</field>
	</section>
</contents>
