				<paint_function   name="paintAssetTypes"   />
				<process_function name="processAssetTypes" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintRegenerate"   />
				<process_function name="processRegenerate" />
			</function_call>
		</field>
